<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:gwas='urn:import:com.gmi.rnaseqwebapp.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
     <ui:style>
         .container {
			margin-left:5pt;
			float:left;
		}
    </ui:style>
	<g:DockLayoutPanel unit="EM" width="100%" height="100%">
		<g:west size="10.0">
			<g:StackLayoutPanel unit="EM">
				<g:stack>
					<g:header size='2'>Genes</g:header>
					<g:Label>Search</g:Label>
				</g:stack>
			</g:StackLayoutPanel>
		</g:west>
		<g:center>
			<gwas:SlidingPanel ui:field="contentContainer" addStyleNames="{style.container}">
			</gwas:SlidingPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>
