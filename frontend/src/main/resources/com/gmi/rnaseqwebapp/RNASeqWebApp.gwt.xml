<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='rnaseqwebapp'>
	<inherits name="com.google.gwt.user.User" />
       <inherits name="com.google.gwt.uibinder.UiBinder" />
	<inherits name="com.google.gwt.inject.Inject" />
	<inherits name='com.gwtplatform.mvp.Mvp' />
	<inherits name='com.gwtplatform.dispatch.Dispatch' />
	<inherits name="at.gmi.nordborglab.widgets.geneviewer.GeneViewer" />
	<inherits name="at.gmi.nordborglab.widgets.gwasgeneviewer.GWASGeneViewer" />
	<inherits name="at.gmi.nordborglab.widgets.geochart.GeoChart" />
	<inherits name="com.google.gwt.resources.Resources" />
	<inherits name='com.google.gwt.visualization.Visualization' />
	<inherits name='com.google.gwt.maps.Maps' />
	<inherits name='com.google.gwt.user.theme.clean.Clean'/>
        <inherits name="name.pehl.piriti.json.JSON" />
	<source path="client" />

	<define-configuration-property name="gin.ginjector"
		is-multi-valued="false" />
	<set-configuration-property name="gin.ginjector"
		value="com.gmi.rnaseqwebapp.client.gin.ClientGinjector" />
	<entry-point class="com.gmi.rnaseqwebapp.client.RNASeqWebApp" />
	
	<script src="http://maps.google.com/maps/api/js?sensor=false" />
	
</module>
